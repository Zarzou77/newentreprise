{% extends 'base.html.twig' %}
{% block body %}

<table class="col-md-4 mx-auto text-center border border-dark mb-3 p-3 mt-3 mr-1">
<thead>
		<tr>
            <th scope="col">Id</th>
            <th scope="col">Prenom</th>
            <th scope="col">Nom</th>
            <th scope="col">Telephone</th>
            <th scope="col">Email</th>
            <th scope="col">Adresse</th>
            <th scope="col">Salaire</th>
            <th scope="col">poste</th>
            <th scope="col">DateDeNaissance</th>
            
              
        </tr>
	</thead>
	<tbody>
    {% for employe in employes %}
        
    
          <tr>
            <th scope="col">{{ employe.id }}</th>
            <td scope="col">{{ employe.prenom }}</td>
            <td scope="col">{{ employe.nom }}</td>
            <td scope="col">{{ employe.telephone }}</td>
            <td scope="col">{{ employe.email }}</td>
            <td scope="col">{{ employe.adresse }}</td>
            <th scope="col">{{ employe.salaire }}</th>
            <td scope="col">{{ employe.poste }}</td>
            <th scope="col">{{ employe.dateDeNaissance }}</th>
            
        </tr>
    {% endfor %}
    </tbody>

</table>
{% endblock %}